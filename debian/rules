#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

export QMAKE_LIBDIR=/usr/lib

override_dh_auto_configure:
	qmake LIBDIR=/usr/lib "QMAKE_CXXFLAGS=$(CFLAGS)" accounts-qt.pro

override_dh_install:
	dh_install --list-missing

override_dh_auto_test:

%:
	dh $@ 
